{
  "name": "application",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.2",
    "jest-expo": "~27.0.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "^16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "lint": "esw ./src/**/*.js",
    "lint-watch": "esw -w --changed ./src/**/*.js",
    "start": "npm run lint && react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "cov": "jest --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "setupTestFrameworkScriptFile": "<rootDir>/enzymeSetup.js",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|expo|@expo|react-navigation|native-base-shoutem-theme|native-base|@shoutem/theme|@shoutem/animation|@shoutem/ui|tcomb-form-native)"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "expo": "^27.0.1",
    "firebase": "^5.3.1",
    "jest-mock": "^23.2.0",
    "moment": "^2.22.2",
    "native-base": "^2.7.1",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-dom": "^16.5.0",
    "react-native": "~0.55.2",
    "react-native-drawer": "^2.5.0",
    "react-native-swiper": "^1.5.13",
    "react-navigation": "^2.11.2",
    "react-navigation-redux-helpers": "^2.0.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga": "^0.16.0",
    "redux-saga-test-plan": "^3.7.0"
  }
}
